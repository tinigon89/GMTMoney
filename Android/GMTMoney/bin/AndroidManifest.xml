        <activity
            android:name="com.teamios.info.gmtmoney.activity.ForgotActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.MyScreenTranNorman" />
        <activity
            android:name="com.teamios.info.gmtmoney.activity.AccountActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.MyScreenTranNorman" />
        <activity
            android:name="com.teamios.info.gmtmoney.activity.NewsActivity"
