            android:theme="@style/Theme.MyScreenTranNorman" />
        <activity
            android:name="com.teamios.info.gmtmoney.activity.SuccessActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.MyScreenTranNorman" />

        <receiver
            android:name="com.google.android.gcm.GCMBroadcastReceiver"
            android:permission="com.google.android.c2dm.permission.SEND" >
            <intent-filter>

                <!-- Receives the actual messages. -->
                <action android:name="com.google.android.c2dm.intent.RECEIVE" />
                <!-- Receives the registration id. -->
                <action android:name="com.google.android.c2dm.intent.REGISTRATION" />

                <category android:name="com.teamios.info.gmtmoney" />
            </intent-filter>
        </receiver>

        <service android:name="com.teamios.info.gmtmoney.service.GCMIntentService" />
    </application>

