<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.teamios.info.gmtmoney"
    android:versionCode="1"
    android:versionName="1.1" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="15" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" >
    </uses-permission>
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <permission
        android:name="com.teamios.info.gmtmoney.permission.C2D_MESSAGE"
        android:protectionLevel="signature" />

    <uses-permission android:name="com.teamios.info.gmtmoney.permission.C2D_MESSAGE" />
    <uses-permission android:name="com.google.android.c2dm.permission.RECEIVE" />
    <uses-permission android:name="android.permission.CAMERA" />

    <uses-feature android:name="android.hardware.camera" />
    <uses-feature android:name="android.hardware.camera.autofocus" />

    <application
        android:icon="@drawable/ic_launcher"
        android:theme="@android:style/Theme.Black.NoTitleBar" >
        <activity
            android:name="com.teamios.info.gmtmoney.activity.HomeActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.teamios.info.gmtmoney.activity.WebViewScreenActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.MyScreenTranNorman" />
        <activity
            android:name="com.teamios.info.gmtmoney.activity.BankingDetailActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.MyScreenTranNorman" />
        <activity
            android:name="com.teamios.info.gmtmoney.activity.ContactUsActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.MyScreenTranNorman" />
        <activity
            android:name="com.teamios.info.gmtmoney.activity.MoreActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.MyScreenTranNorman" />
        <activity
            android:name="com.teamios.info.gmtmoney.activity.DailyRatesActivity"
            android:configChanges="keyboardHidden"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.MyScreenTranNorman"
            android:windowSoftInputMode="stateHidden" />
        <activity
            android:name="com.teamios.info.gmtmoney.activity.LoginActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.MyScreenTranNorman" />
        <activity
